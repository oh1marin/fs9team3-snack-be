# Server Configuration
PORT=3001
# 프론트 주소 (쉼표로 여러 개). 배포 시 백엔드와 다른 도메인이면 필수
# CORS_ORIGIN=http://localhost:3000,http://your-front.com
# HTTPS 사용 시에만 true (EC2에서 http만 쓰면 설정 안 함)
# USE_HTTPS=true

# Database Configuration
# postgresql://USERNAME:PASSWORD@HOST:PORT/DATABASE_NAME?schema=public
DATABASE_URL=postgresql://username:password@localhost:5432/snack_db?schema=public

# JWT Configuration
JWT_SECRET=your-secret-key-change-this-in-production
JWT_REFRESH_SECRET=your-refresh-secret-key-change-this-in-production

# -- AWS / SSH (필요 시 주석 해제 후 값 입력) --
# SSH_KEY_PATH=
# EC2_HOST=
# EC2_USER=
# DB_HOST=
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_REGION=ap-northeast-2
# AWS_PUBLIC_BUCKET_NAME=
# AWS_PRIVATE_BUCKET_NAME=

NODE_ENV=development
